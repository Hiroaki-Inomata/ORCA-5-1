              IF   ( RECE62-KOHNUM(1)  =   "974" )
              OR   ( RECE62-KOHNUM(2)  =   "974" )
              OR   ( RECE62-KOHNUM(3)  =   "974" )
              OR   ( RECE62-KOHNUM(4)  =   "974" )
                 IF   WRK-GEN70-GENDOGAKU  >  20000
                     MOVE  20000  TO   WRK-GEN70-GENDOGAKU
                 END-IF
              END-IF
