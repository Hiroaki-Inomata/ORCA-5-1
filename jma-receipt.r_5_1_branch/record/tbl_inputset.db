tbl_inputset {
	HOSPNUM				number(2,0);
	SETCD   			char(6);
	YUKOSTYMD  			char(8);
	YUKOEDYMD	  		char(8);
	SETSEQ  			number(3);
	INPUTCD				char(9);
	SURYO1  			number(12,5);
	SURYO2				number(12,5);
	KAISU				number(3);
	COMENT				varchar(140);
	ATAI1				char(10);
	ATAI2				char(10);
	ATAI3				char(10);
	ATAI4				char(10);
	ATAI5				char(10);
	INPUTKBN			char(1);
	KANSURYO			number(10,5);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPNUM,SETCD,YUKOSTYMD,YUKOEDYMD,SETSEQ;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_inputset_key_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			AND   SETSEQ      =   :SETSEQ
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_inputset_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			ORDER   BY        SETSEQ
		;
	};
};
path	del11	{
	DBDELETE	{
		DELETE
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD   =   :YUKOSTYMD
			AND   YUKOEDYMD   =   :YUKOEDYMD
		;
	};
};
path	all 	{
	DBSELECT	{
		DECLARE tbl_inputset_all_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			ORDER   BY        SETCD,
                                          YUKOSTYMD,
                                          YUKOEDYMD,
                                          SETSEQ
 		;
	};
};

