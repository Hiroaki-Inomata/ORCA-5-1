tbl_etensu_1_master {
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	H_TANI1				number(2,0);
	H_GROUP1			varchar(7);
	H_TANI2				number(2,0);
	H_GROUP2			varchar(7);
	H_TANI3				number(2,0);
	H_GROUP3			varchar(7);
	R_DAY				number(1,0);
	R_MONTH				number(1,0);
	R_SAME				number(1,0);
	R_WEEK				number(1,0);
	N_GROUP				number(3,0);
	C_KAISU				number(1,0);
	CHGYMD   			char(8);
#-----> 主キー更新用項目
	UPSRYCD				char(9),virtual;
	UPYUKOSTYMD			char(8),virtual;
	UPYUKOEDYMD			char(8),virtual;
};
primary	{
	SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_1_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_1
			WHERE   SRYCD     =  :SRYCD
 			AND     YUKOSTYMD <= :YUKOSTYMD
			AND     YUKOEDYMD >= :YUKOEDYMD
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_etensu_1
			VALUES(
				:SRYCD,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:H_TANI1,
				:H_GROUP1,
				:H_TANI2,
				:H_GROUP2,
				:H_TANI3,
				:H_GROUP3,
				:R_DAY,
				:R_MONTH,
				:R_SAME,
				:R_WEEK,
				:N_GROUP,
				:C_KAISU,
				:CHGYMD
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_etensu_1
			SET
				SRYCD     = :SRYCD,
				YUKOSTYMD = :YUKOSTYMD,
				YUKOEDYMD = :YUKOEDYMD,
				H_TANI1   = :H_TANI1,
				H_GROUP1  = :H_GROUP1,
				H_TANI2   = :H_TANI2,
				H_GROUP2  = :H_GROUP2,
				H_TANI3   = :H_TANI3,
				H_GROUP3  = :H_GROUP3,
				R_DAY     = :R_DAY,
				R_MONTH   = :R_MONTH,
				R_SAME    = :R_SAME,
				R_WEEK    = :R_WEEK,
				N_GROUP   = :N_GROUP,
				C_KAISU   = :C_KAISU,
				CHGYMD    = :CHGYMD
			WHERE   SRYCD     = :UPSRYCD
			AND     YUKOSTYMD = :UPYUKOSTYMD
			AND     YUKOEDYMD = :UPYUKOEDYMD
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_etensu_1
			WHERE   SRYCD     = :SRYCD
 			AND     YUKOSTYMD = :YUKOSTYMD
			AND     YUKOEDYMD = :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_1_master_key2_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_1
			WHERE   SRYCD = :SRYCD
			ORDER   BY	YUKOSTYMD
		;
	};
};
