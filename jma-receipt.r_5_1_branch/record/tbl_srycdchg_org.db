tbl_srycdchg_org {
	HOSPNUM			number(2,0);
	IPNSRYCD   		char(9);
	RJNSRYCD  		char(9);
	TERMID  		varchar(16);
	OPID    		varchar(16);
	CREYMD  		char(8);
	UPYMD   		char(8);
	UPHMS			char(6);
};
primary	{
	HOSPNUM,IPNSRYCD,RJNSRYCD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_srycdchg_org_key_csr CURSOR FOR
		SELECT *
		FROM tbl_srycdchg_org
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   IPNSRYCD  =   :IPNSRYCD
			AND   RJNSRYCD    =   :RJNSRYCD
		;
	};
};

