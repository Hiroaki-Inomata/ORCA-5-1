path	key 	{
	DBSELECT	{
		DECLARE tbl_syunou_prv_key_csr CURSOR FOR
		SELECT *
		FROM tbl_syunou_prv
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   DENPNUM     =   :DENPNUM
