tbl_access_key {
	HOSPNUM		number(2,0);
	ACCESS_KEY_1	varchar(128);
	CREYMD		char(8);
	UPHMS		char(6);
	KADO_YM		varchar(6);
	KADO_LEVEL	number(1,0);
	CONTRACT_COND	number(1,0);
	CONTRACT_YMD	varchar(8);
	INQUIRE_YMD	varchar(8);
};

primary	{
	HOSPNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_access_key_key_csr CURSOR FOR
		SELECT *
		FROM tbl_access_key
		WHERE HOSPNUM = :HOSPNUM
		;
	};
};

path	del1 	{
	DBDELETE	{
		DELETE
		FROM tbl_access_key
		WHERE HOSPNUM = :HOSPNUM
		;
	};
};
