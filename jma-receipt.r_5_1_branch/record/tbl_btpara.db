path	ins1	{
	DBINSERT	{
		INSERT  INTO
		tbl_btpara (
			HOSPNUM,
			SHELLID,
			RUN_OPID,
			DSP_RENNUM,
			RENNUM,
			JOBID,
			SCRIPTID,
			INFO_KBN,
			INFO_PARA,
			TERMID,
			OPID,
			CREYMD,
			UPYMD,
			UPHMS
