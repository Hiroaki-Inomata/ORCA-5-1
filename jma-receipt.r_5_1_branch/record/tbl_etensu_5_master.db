tbl_etensu_5_master {
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	TANICD				number(3,0);
	TANINAME			varchar(12);
	KAISU				number(3,0);
	TOKUREI				number(1,0);
	CHGYMD   			char(8);
#-----> 主キー更新用項目
	UPSRYCD				char(9),virtual;
	UPYUKOSTYMD			char(8),virtual;
	UPYUKOEDYMD			char(8),virtual;
	UPTANICD			number(3,0),virtual;
};
primary	{
	SRYCD,TANICD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_5_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_5
			WHERE   SRYCD     =  :SRYCD
 			AND     TANICD    =  :TANICD
 			AND     YUKOSTYMD <= :YUKOSTYMD
			AND     YUKOEDYMD >= :YUKOEDYMD
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_etensu_5
			VALUES(
				:SRYCD,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:TANICD,
				:TANINAME,
				:KAISU,
				:TOKUREI,
				:CHGYMD
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_etensu_5
			SET
				SRYCD     = :SRYCD,
				YUKOSTYMD = :YUKOSTYMD,
				YUKOEDYMD = :YUKOEDYMD,
				TANICD    = :TANICD,
				TANINAME  = :TANINAME,
				KAISU     = :KAISU,
				TOKUREI   = :TOKUREI,
				CHGYMD    = :CHGYMD
			WHERE 	SRYCD     = :UPSRYCD
 			AND     TANICD    = :UPTANICD
			AND	YUKOSTYMD = :UPYUKOSTYMD
			AND	YUKOEDYMD = :UPYUKOEDYMD
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_etensu_5
			WHERE   SRYCD     = :SRYCD
 			AND     TANICD    = :TANICD
 			AND     YUKOSTYMD = :YUKOSTYMD
			AND     YUKOEDYMD = :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_5_master_key2_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_5
			WHERE   SRYCD  = :SRYCD
 			AND     TANICD = :TANICD
			ORDER   BY	YUKOSTYMD
		;
	};
};
