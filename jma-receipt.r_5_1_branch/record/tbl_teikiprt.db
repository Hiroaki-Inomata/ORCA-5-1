tbl_teikiprt	{
        HOSPNUM              number(2,0);
        BTUNUM              char(2);
        DENPNUM             number(7);
        PTID                number(10);
        PTNUM               char(20);
        NAME                char(100);
};

primary	{
	HOSPNUM,BTUNUM,DENPNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_teikiprt_key_csr CURSOR FOR
		SELECT *
		FROM tbl_teikiprt
        WHERE   HOSPNUM     =   :HOSPNUM
        AND	BTUNUM      =   :BTUNUM
		AND	DENPNUM     =   :DENPNUM
		;
	};
};

path	key2	{ 	
	DBSELECT	{
		DECLARE tbl_teikiprt_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_teikiprt
        	WHERE   HOSPNUM     =   :HOSPNUM
		ORDER   BY BTUNUM,NAME
		;
	};
};

