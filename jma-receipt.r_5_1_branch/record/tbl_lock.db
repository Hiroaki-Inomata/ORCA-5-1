path	key		{ 	
	DBSELECT	{
		DECLARE tbl_lock_key_csr CURSOR FOR
		SELECT *
		FROM tbl_lock
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   LOCK_TERMID		=   :LOCK_TERMID 
			ORDER   BY        LOCK_TERMID 
		;
	};
};

