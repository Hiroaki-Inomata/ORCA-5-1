};

path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_prvkanri_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_prvkanri
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND   TBL_KEY      =   :TBL_KEY
                    AND     RENNUM       =   :RENNUM
                    AND     TBL_GROUP    =   :TBL_GROUP
                    AND     TBL_UUID     =   :TBL_UUID
                    AND     SHORI_RENNUM =   :SHORI_RENNUM 
                    AND     PAGE         >=  :STPAGE
                    AND     PAGE         <=  :EDPAGE
                    ORDER BY TBL_KEY,RENNUM,TBL_GROUP,TBL_UUID,SHORI_RENNUM,PAGE    
		;
	};
};

