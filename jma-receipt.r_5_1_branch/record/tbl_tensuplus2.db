path	key 	{
	DBSELECT	{
		DECLARE tbl_tensuplus2_key_csr CURSOR FOR
		SELECT *
		FROM tbl_tensuplus2
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   SRYCD    =   :SRYCD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_tensuplus2_all_csr CURSOR FOR
		SELECT *
		FROM tbl_tensuplus2
		WHERE 	HOSPNUM	= :HOSPNUM
 		;
	};
};

