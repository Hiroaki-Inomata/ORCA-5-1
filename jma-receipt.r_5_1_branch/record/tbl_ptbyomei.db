path	key9		{ 	
	DBSELECT	{
		DECLARE tbl_ptbyomei_key9_csr CURSOR FOR
		SELECT *
		FROM tbl_ptbyomei
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   PTID        =   :PTID
			AND   SRYYMD      <   :SRYYMD
			AND   DLTFLG      =   ' '
			AND  ((TENKIKBN    =   :TENKIKBN )
                          OR (TENKIYMD    =
                           (SELECT MAX(TENKIYMD)
                                   FROM tbl_ptbyomei 
                               	WHERE   HOSPNUM    =   :HOSPNUM
		        	AND   PTID        =   :PTID
                                AND   SRYYMD      <   :SRYYMD
                                AND   TENKIKBN    =   '1'
		         	AND   DLTFLG      =   ' ' ))
                          OR (TENKIYMD    =
                           (SELECT MAX(TENKIYMD)
                                   FROM tbl_ptbyomei 
                               	WHERE   HOSPNUM    =   :HOSPNUM
		        	AND   PTID        =   :PTID
                                AND   SRYYMD      <   :SRYYMD
                                AND   TENKIKBN    =   '3'
		         	AND   DLTFLG      =   ' ' )))
			ORDER  BY  TENKIKBN DESC,
                                   SRYYMD
		;
	};
};

