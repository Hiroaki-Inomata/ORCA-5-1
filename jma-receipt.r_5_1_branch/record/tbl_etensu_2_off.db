tbl_etensu_2_off {
	HOSPNUM				number(2,0);
	H_GROUP 			varchar(7);
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	TERMID				varchar(16);
	OPID				varchar(16);
	CREYMD				char(8);
	UPYMD				char(8);
	UPHMS				char(6);
};
primary	{
	HOSPNUM,H_GROUP,SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_2_off_key_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_2_off
		WHERE   HOSPNUM  =   :HOSPNUM
		AND     H_GROUP  =   :H_GROUP
		AND     SRYCD    =   :SRYCD
 		AND   YUKOSTYMD  <=  :YUKOSTYMD
		AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_2_off_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_2_off
		WHERE   HOSPNUM  =   :HOSPNUM
		AND     H_GROUP  =   :H_GROUP
		AND	YUKOSTYMD  <=  :YUKOSTYMD
		AND	YUKOEDYMD  >=  :YUKOEDYMD
			ORDER   BY  SRYCD,
				    YUKOSTYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_etensu_2_off_all_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_2_off
			ORDER   BY  H_GROUP,
				    SRYCD,
				    YUKOSTYMD
 		;
	};
};
