primary	{
        HOSPNUM,PTID,PTNUM,NYUGAIKBN,SRYKA,HKNCOMBI,SYSYMD,
        PRINTYMD,PRINTHMS,PRTKBN,ORDERNUM,RENNUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_orderprt_key_csr CURSOR FOR
		SELECT *
		FROM tbl_orderprt
			WHERE HOSPNUM       =   :HOSPNUM
			AND   PTID         =   :PTID
			AND   PTNUM        =   :PTNUM
			AND   NYUGAIKBN    =   :NYUGAIKBN
			AND   SRYKA        =   :SRYKA
			AND   HKNCOMBI     =   :HKNCOMBI
			AND   SYSYMD       =   :SYSYMD
			AND   PRINTYMD     =   :PRINTYMD
			AND   PRINTHMS     =   :PRINTHMS
			AND   PRTKBN       =   :PRTKBN
			AND   ORDERNUM     =   :ORDERNUM
			AND   RENNUM       =   :RENNUM
		;
	};
};

