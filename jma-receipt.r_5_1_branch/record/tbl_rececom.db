path	upd1	{ 	
	DBUPDATE	{
		UPDATE	tbl_rececom 
				SET	
                   HOSPNUM             = :HOSPNUM,
                   PTID                = :PTID,
		   NYUGAIKBN           = :NYUGAIKBN,
                   SRYKA               = :SRYKA,
                   SRYYM               = :SRYYM,
                   HKNCOMBI            = :HKNCOMBI,
                   SRYDD               = :SRYDD,
                   SJKBN               = :SJKBN,
                   RENNUM              = :RENNUM,
                   COMMENT             = :COMMENT,
                   TERMID              = :TERMID,
                   OPID                = :OPID,
                   CREYMD              = :CREYMD,
                   UPYMD               = :UPYMD,
                   UPHMS               = :UPHMS
               WHERE   HOSPNUM       =   :UPHOSPNUM
                 AND   PTID          =   :UPPTID
 	  	 AND   NYUGAIKBN     =   :UPNYUGAIKBN
                 AND   SRYKA         =   :UPSRYKA 
                 AND   SRYYM         =   :UPSRYYM
                 AND   HKNCOMBI      =   :UPHKNCOMBI
                 AND   SRYDD         =   :UPSRYDD
                 AND   SJKBN         =   :UPSJKBN
                 AND   RENNUM        =   :UPRENNUM
		;
	};
};

