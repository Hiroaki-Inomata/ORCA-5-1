tbl_prtctrl	{
	HOSPNUM			number(2,0);
	TBL_KEY			char(8);
	RENNUM  		number(4);
	TBL_GROUP		char(14);
	TBL_UUID 		char(36);
	SHORI_RENNUM		number(4);
	PAGE    		number(5);
	DATA_KBN		number(2,0);
	CONT_FLG		number(2,0);
	GYOUMU_CD               number(3);
	DNAME			varchar(200);
	TERMID			varchar(64);
	OPID			varchar(16);
	CREYMD 			varchar(8);
	CREHMS 			varchar(6);
#-----> virtual 項目
	SELLIMIT		number(8),virtual;
	SELOFFSET		number(8),virtual;
};

primary	{
	HOSPNUM,TBL_KEY,RENNUM,TBL_GROUP,TBL_UUID, SHORI_RENNUM, PAGE;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_prtctrl_key_csr CURSOR FOR
		SELECT *
		FROM tbl_prtctrl
		WHERE	HOSPNUM		=	:HOSPNUM
                    AND	TBL_KEY		=	:TBL_KEY
                    AND	RENNUM		=	:RENNUM
                    AND	TBL_GROUP	=	:TBL_GROUP
                    AND	TBL_UUID	=	:TBL_UUID
                    AND	SHORI_RENNUM	=	:SHORI_RENNUM 
                    AND	PAGE		=	:PAGE
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_prtctrl_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_prtctrl
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND	TBL_KEY		=	:TBL_KEY
                    AND	RENNUM		=	:RENNUM
                    AND	TBL_GROUP	=	:TBL_GROUP
                    AND	TBL_UUID	=	:TBL_UUID
                    AND	SHORI_RENNUM	=	:SHORI_RENNUM 
                    ORDER BY  PAGE
		;
	};
};


path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_prtctrl_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_prtctrl
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND	TBL_KEY		=	:TBL_KEY
                    AND	RENNUM		=	:RENNUM
                    AND	TBL_GROUP	=	:TBL_GROUP
                    AND	TBL_UUID	=	:TBL_UUID
                    AND	SHORI_RENNUM	<>	99	
                    ORDER BY  PAGE
		;
	};
};

path	key4		{ 	
	DBSELECT	{
		DECLARE tbl_prtctrl_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_prtctrl
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND	TBL_KEY		=	:TBL_KEY
                    AND	RENNUM		=	:RENNUM
                    AND	TBL_GROUP	=	:TBL_GROUP
                    AND	TBL_UUID	=	:TBL_UUID
                    ORDER BY  SHORI_RENNUM
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_prtctrl_all_csr CURSOR FOR
		SELECT *
		FROM tbl_prtctrl
		WHERE 	HOSPNUM	= :HOSPNUM
 		;
	};
};

path	del11	{
	DBDELETE	{
		DELETE
		FROM	tbl_prtctrl
		WHERE	HOSPNUM		=	:HOSPNUM
			AND	TBL_KEY	=	:TBL_KEY
			AND	RENNUM	=	:RENNUM
			AND	TBL_GROUP	=	:TBL_GROUP
			AND	TBL_UUID	=	:TBL_UUID
			AND	DATA_KBN	=	:DATA_KBN
			AND	SHORI_RENNUM	=	:SHORI_RENNUM
                ;
        };
};


