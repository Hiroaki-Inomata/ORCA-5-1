path	key		{ 	
	DBSELECT	{
		DECLARE tbl_kohsky_key_csr CURSOR FOR
		SELECT *
		FROM tbl_kohsky
                WHERE   HOSPNUM       =   :HOSPNUM
                  AND   SRYYM        =   :SRYYM
                  AND   NYUGAIKBN    =   :NYUGAIKBN
                  AND   PTID         =   :PTID
                  AND   RECEKA       =   :RECEKA 
                  AND   TEISYUTUSAKI =   :TEISYUTUSAKI
                  AND   RECESYUBETU  =   :RECESYUBETU
                  AND   HKNJANUM_KEY =   :HKNJANUM_KEY
                  AND   HOJOKBN_KEY  =   :HOJOKBN_KEY
                  AND   KOHID_KEY    =   :KOHID_KEY 
                  AND   TEKSTYMD     =   :TEKSTYMD
                  AND   RECEIPT_KBN    =   :RECEIPT_KBN
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_kohsky_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_kohsky
                WHERE (      HOSPNUM      =   :HOSPNUM     )
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )))
		;
	};
};

