primary	{
	HOSPNUM,CDSYU,BYOMEIINPUTCD;
};

path	key	{ 	
	DBSELECT	{
		DECLARE tbl_userbyomei_key_csr CURSOR FOR
		SELECT *
		FROM tbl_userbyomei
			WHERE HOSPNUM          =   :HOSPNUM
			AND   CDSYU           =   :CDSYU 
			AND   BYOMEIINPUTCD   =   :BYOMEIINPUTCD 
		;

	};
};

path	key2	{ 	
	DBSELECT	{
		DECLARE tbl_userbyomei_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_userbyomei
			WHERE HOSPNUM          =   :HOSPNUM
			AND   BYOMEI       LIKE :BYOMEI
		;
	};
};

path	key3	{ 	
	DBSELECT	{
		DECLARE tbl_userbyomei_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_userbyomei
			WHERE HOSPNUM          =   :HOSPNUM
                    AND (      CDSYU      =  'A'                   
                          OR   CDSYU      =  'J'        )
                    ORDER  BY  DSPSEQ,BYOMEIINPUTCD       
		;
	};
};

