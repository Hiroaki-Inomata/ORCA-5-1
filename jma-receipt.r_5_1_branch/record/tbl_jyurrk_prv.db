};

path	key 	{
	DBSELECT	{
		DECLARE tbl_jyurrk_prv_key_csr CURSOR FOR
		SELECT *
		FROM tbl_jyurrk_prv
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   PTID        =   :PTID
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   SRYKA       =   :SRYKA
			AND   SRYYMD      =   :SRYYMD
			AND   RENNUM      =   :RENNUM
			AND   DOUJI_RENNUM =  :DOUJI_RENNUM
                	AND   KAIKEI_RENNUM = :KAIKEI_RENNUM
			AND   EDANUM      =   :EDANUM
		;
	};
};

