tbl_receprt	{
	HOSPNUM				number(2,0);
        PRTID               char(4);
        RECEYM              char(6);
        CREYMD              char(8);
        CREHMS              char(6);
        TEISYUTUSAKI        char(1);
        SRYKA               char(2);
        PREFKBN_SRT         char(2);
        HKNJANUM_SRT        char(1);
        HKNJANUM            char(8);
        TEKSTYMD            char(8);
        RECESYUBETU_SRT     char(3);
        RECESYUBETU         char(4);
        PREFKBN             char(1);
        PREFNUM             char(2);
        PTNUM               char(20);
        SRYYM               char(6);
        RENNUM              number(3);
        RECENUM             number(6);
        HKNKBN              char(1);
        KANANAME            char(100);
        PRTJYUN             number(2);
        PRTRENNUM           number(6);
        NYUGAIKBN           char(1);
        KOUGAKU             char(1);
        ZAITAKU             char(1);
        CHOUKI              char(1);
        HKNJANUM_S          char(8);
        SYUBETU_SRT1        integer;
        GRP_NO1             number(3);
        HONKZKKBN           char(1); 
        KOUHI_NO            number(3);
        SYUBETU_UNDER_1     varchar(80);
        GRP_NO2             char(3);
        SYUBETU_UNDER_2     varchar(40);
        SYUBETU_SRT2        integer;
        DRCD                char(5);
        DRCD_SRYKA          char(2);
        SORT_1              varchar(10);
        SORT_2              varchar(50);
        FORMID              char(20);
        PRTDATA             varchar(10000);
        PRTNM               char(16);
        TERMID              char(64);
        OPID                char(16);
        PRTJYUN_SYUKA       number(4);
#-----> 検索用項目
	STKANANAME			char(100),virtual;
	EDKANANAME			char(100),virtual;
	STRENNUM			number(2),virtual;
	EDRENNUM			number(2),virtual;
};

primary	{
	HOSPNUM,PRTID,RECEYM,
CREYMD,CREHMS,TEISYUTUSAKI,SRYKA,PREFKBN_SRT,HKNJANUM_SRT,HKNJANUM,RECESYUBETU,TEKSTYMD,ZAITAKU,CHOUKI,PTNUM,SRYYM,RENNUM,RECENUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_receprt_key_csr CURSOR FOR
		SELECT *
		FROM tbl_receprt
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND   PRTID        =   :PRTID
                    AND     RECEYM       =   :RECEYM
                    AND     CREYMD       =   :CREYMD
                    AND     CREHMS       =   :CREHMS
                    AND     TEISYUTUSAKI =   :TEISYUTUSAKI
                    AND     RECESYUBETU  =   :RECESYUBETU
                    AND     PREFKBN      =   :PREFKBN
                    AND     KANANAME     >=  :STKANANAME
                    AND     KANANAME     <=  :EDKANANAME 
                   ORDER BY  PREFKBN,PREFNUM,HKNKBN,HKNJANUM,KANANAME,PTNUM,SRYYM,RENNUM
		;
	};
};

