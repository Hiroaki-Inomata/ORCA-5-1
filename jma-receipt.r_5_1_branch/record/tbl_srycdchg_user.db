tbl_srycdchg_user {
	HOSPNUM			number(2,0);
	IPNSRYCD   		varchar(9);
	RJNSRYCD  		varchar(9);
	TERMID  		varchar(16);
	OPID    		varchar(16);
	CREYMD  		varchar(8);
	UPYMD   		varchar(8);
	UPHMS			varchar(6);
#-----> 主キー更新用
	UPHOSPNUM		number(2,0),virtual;
	UPIPNSRYCD   		varchar(9),virtual;
	UPRJNSRYCD  		varchar(9),virtual;
};
primary	{
	HOSPNUM,IPNSRYCD,RJNSRYCD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_srycdchg_user_key_csr CURSOR FOR
		SELECT *
		FROM tbl_srycdchg_user
		WHERE	HOSPNUM = :HOSPNUM
			AND IPNSRYCD = :IPNSRYCD
			AND RJNSRYCD = :RJNSRYCD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_srycdchg_user_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_srycdchg_user
		WHERE	HOSPNUM = :HOSPNUM
			AND IPNSRYCD = :IPNSRYCD
		;
