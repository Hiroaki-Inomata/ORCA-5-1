path	key 	{
	DBSELECT	{
		DECLARE tbl_syutotal_prv_key_csr CURSOR FOR
		SELECT *
		FROM tbl_syutotal_prv
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   DENPNUM     =   :DENPNUM
			AND   CREYMD      =   :CREYMD
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_syutotal_prv_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_syutotal_prv
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			ORDER BY CREYMD DESC
		;
	};
};
path	all 	{
	DBSELECT	{
		DECLARE tbl_syutotal_prv_all_csr CURSOR FOR
		SELECT *
		FROM tbl_syutotal_prv
			WHERE   HOSPNUM    =   :HOSPNUM
 		;
	};
};

path	del1 	{
