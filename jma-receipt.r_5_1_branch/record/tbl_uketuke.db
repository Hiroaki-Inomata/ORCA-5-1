tbl_uketuke {
	HOSPNUM				number(2,0);
	UKEYMD  			char(8);
	UKEID    			number(5,0);
	UKETIME 			char(6);
	PTID	   			number(10,0);
	NAME	 			varchar(100);
	SRYKA   			char(2);
	DRCD    			char(5);
	SRYNAIYO			char(2);
	HKNCOMBI			number(4,0);
	KAIKEITIME			number(4,0);
	KEYYYKTIME			number(4,0);
	YYKTIME 			number(4,0);
	YYKID   			number(2);
	HYOJIJUN  			number(4);
	KAKREN  			char(2);
	SRFLG    			char(2);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPNUM,UKEYMD,UKEID;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_uketuke_key_csr CURSOR FOR
		SELECT *
		FROM tbl_uketuke
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   UKEYMD      =   :UKEYMD
			AND   UKEID       =   :UKEID
		;
	};
};

