		FROM view_bd002
			WHERE   HOSPNUM      =   :HOSPNUM
                    AND     NYUGAIKBN   =   :NYUGAIKBN 
                    AND     DENPPRTYMD  >=  :DENPPRTYMD 
                    ORDER BY  DENPNUM
		;
	};
};


