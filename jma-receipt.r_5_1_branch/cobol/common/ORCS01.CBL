                 ELSE
                   MOVE ZERO  TO   FLG-CYOKI-HANTEI9
                 END-IF
               END-IF
               IF  (LNK-S01-SAIKEISAN-KBN = "1"              )
               AND (WK-SYUNOU-GENBUTU-SECMONEY-MAE
                                          = ZERO OR 9        )
               AND (LNK-SYU-TOTAL-HKNTEN  =
                               WK-SYUNOU-GENBUTU-TOTALTEN-MAE)
                 MOVE   ZERO  TO   FLG-CYOKI-HANTEI9
                 IF  WK-SYUNOU-GENBUTU-SECMONEY-MAE = 9
                   MOVE  9    TO   LNK-SYU-SECMONEY
                 END-IF
               END-IF
               IF  (LNK-SYU-SECMONEY   =  1 OR 2 OR 3 OR 4 OR 6)
               OR  (FLG-CYOKI-HANTEI9  =  ZERO                 )
                 CONTINUE
               ELSE
                 MOVE   "1"   TO   LNK-S01-GENBUTU-MSG-FLG
                 MOVE    5    TO   LNK-SYU-SECMONEY
               END-IF
           END-IF
      *
           IF  (LNK-S01-IKTCYOKIFLG    =  SPACE   )
