                     UNTIL  (IDX24             >  100 )
                      OR    (WRK-HKNNUM(IDX24) = SPACE)
                      OR    (FLG-ARI           =  1   )
             IF  SYU-KOH-HKNNUM(IDX-NEXT) = WRK-HKNNUM(IDX24)
