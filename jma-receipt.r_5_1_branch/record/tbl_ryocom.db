primary	{
	HOSPNUM,NYUGAIKBN,PTID,SRYYM,ZAINUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_ryocom_key_csr CURSOR FOR
		SELECT *
		FROM tbl_ryocom
			WHERE HOSPNUM      =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYYM       =   :SRYYM
			AND   ZAINUM      =   :ZAINUM
		;
	};
};

