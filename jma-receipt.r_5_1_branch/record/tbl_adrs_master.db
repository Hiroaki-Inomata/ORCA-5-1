tbl_adrs_master {
	HOSPNUM				number(2,0);
	LPUBCD				varchar(5);
	POST				varchar(7);
	RENNUM				number(2);
	PREFKANA			varchar(40);
	CITYKANA			varchar(200);
	TOWNKANA			varchar(400);
	PREFNAME			varchar(20);
	CITYNAME			varchar(100);
	TOWNNAME			varchar(200);
	EDITADRS_KANA		varchar(640);
	EDITADRS_NAME		varchar(320);
	TOWNDIVFLG			varchar(1);
	MANYTOWNFLG			varchar(1);
	TOWNNAME2			varchar(1200);
};

primary	{
	HOSPNUM,LPUBCD,POST,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_adrs_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_adrs_master
		WHERE   HOSPNUM = :HOSPNUM
			AND LPUBCD = :LPUBCD
			AND POST   = :POST
			AND RENNUM = :RENNUM
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_adrs_master
			VALUES(
				:LPUBCD,
				:POST,
				:RENNUM,
				:PREFKANA,
				:CITYKANA,
				:TOWNKANA,
				:PREFNAME,
				:CITYNAME,
				:TOWNNAME,
				:EDITADRS_KANA,
				:EDITADRS_NAME,
				:TOWNDIVFLG,
				:MANYTOWNFLG,
				:HOSPNUM,
				:TOWNNAME2
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_adrs_master
			SET
				LPUBCD = :LPUBCD,
				POST   = :POST,
				RENNUM = :RENNUM,
				PREFKANA = :PREFKANA,
				CITYKANA = :CITYKANA,
				TOWNKANA = :TOWNKANA,
				PREFNAME = :PREFNAME,
				CITYNAME = :CITYNAME,
				TOWNNAME = :TOWNNAME,
				EDITADRS_KANA = :EDITADRS_KANA,
				EDITADRS_NAME = :EDITADRS_NAME,
				TOWNDIVFLG = :TOWNDIVFLG,
				MANYTOWNFLG = :MANYTOWNFLG,
				HOSPNUM = :HOSPNUM,
				TOWNNAME2 = :TOWNNAME2
		WHERE   HOSPNUM = :HOSPNUM
			AND LPUBCD = :LPUBCD
			AND POST   = :POST
			AND RENNUM = :RENNUM
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_adrs_master
		WHERE   HOSPNUM = :HOSPNUM
			AND LPUBCD = :LPUBCD
			AND POST   = :POST
			AND RENNUM = :RENNUM
		;
	};
};

path	all		{ 	
	DBSELECT	{
		DECLARE tbl_adrs_master_all_csr CURSOR FOR
		SELECT *
		FROM master.tbl_adrs_master
		WHERE   HOSPNUM = :HOSPNUM
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_adrs_master_key2_csr CURSOR FOR
		SELECT *
		FROM master.tbl_adrs_master
			WHERE (HOSPNUM      =   :HOSPNUM)
			AND   (POST   LIKE :POST)
			ORDER  BY  POST,
                       LPUBCD,
                       RENNUM;
	};
};
path	key7		{ 	
	DBSELECT	{
		DECLARE tbl_adrs_master_key7_csr CURSOR FOR
		SELECT *
		FROM master.tbl_adrs_master
		WHERE   HOSPNUM = :HOSPNUM
			AND POST   = :POST
			AND LPUBCD = :LPUBCD
		ORDER BY RENNUM DESC
		;
	};
};
