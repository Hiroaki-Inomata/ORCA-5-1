primary	{
	HOSPNUM,NYUGAIKBN,PTID,SRYKA,SRYYM,ZAINUM,RENNUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_sryacctplus_key_csr CURSOR FOR
		SELECT *
		FROM tbl_sryacctplus
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYKA       =   :SRYKA
			AND   SRYYM       =   :SRYYM
			AND   ZAINUM      =   :ZAINUM
			AND   RENNUM      =   :RENNUM
		;
	};
};

