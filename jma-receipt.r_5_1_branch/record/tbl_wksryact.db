			AND   HKNCOMBI    =   :HKNCOMBI
			AND   DOUJITSUKBN   =   :DOUJITSUKBN
			AND   ZAINUM      =   :ZAINUM
			AND   RENNUM      =   :RENNUM
		;
	};
};

