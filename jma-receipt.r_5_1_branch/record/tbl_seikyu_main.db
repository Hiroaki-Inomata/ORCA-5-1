path	key3	{
	DBSELECT	{
		DECLARE tbl_seikyu_main_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu_main
                WHERE (      HOSPNUM      =   :HOSPNUM   )
                  AND (      HKNNUM_S    <>  '060'       )
                  AND (      HKNNUM_S    <>  '067'       ) 
                  AND (      HKNNUM_S    <>  '068'       )
                  AND (      HKNNUM_S    <>  '069'       ) 
                  AND (      HKNNUM_S    <>  '039'       ) 
                  AND (      HKNNUM_S    <>  '040'       ) 
                  AND (      HKNNUM_S    <>  '975'       ) 
                  AND (      HKNNUM_S    <>  '971'       ) 
                  AND (      HKNNUM_S    <>  '973'       ) 
                  AND (      HKNNUM_S    <>  '970'       ) 
                  AND (      RECESYUBETU <>  '0000'      )
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )))
		;
	};
};

path	key4	{
	DBSELECT	{
		DECLARE tbl_seikyu_main_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu_main
                WHERE (      HOSPNUM      =   :HOSPNUM   )
                  AND (      HKNNUM_S    =  '060'                   
                        OR   HKNNUM_S    =  '067'        ) 
                  AND (      RECESYUBETU <>  '0000'      ) 
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )) )
                  AND (      HKNJANUM_S  >=  :XXSTHKNJANUM  
                       AND   HKNJANUM_S  <=  :XXEDHKNJANUM  ) 
                ORDER  BY  HKNJANUM_S   
		;
	};
};

