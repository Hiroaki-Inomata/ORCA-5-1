      *療養の給付
      *    件数
           ADD   1                   TO  SKT0400TMP-KENSU(IDX1 IDX2)
                                         SKT0400TMP-GKENSU(IDX2)
      *    日数
           ADD   RECE10-JNISSU(1)    TO  SKT0400TMP-NISSU(IDX1 IDX2)
                                         SKT0400TMP-GNISSU(IDX2)
      *    点数
           ADD   RECE10-TOTALTEN(1)  TO  SKT0400TMP-TENSU(IDX1 IDX2)
                                         SKT0400TMP-GTENSU(IDX2)
      *食事療養
      *    件数
           IF  RECE10-SHOKUJINISSU(1)  >  0
             ADD  1  TO  SKT0400TMP-S-KENSU(IDX1)
                         SKT0400TMP-S-GKENSU
           END-IF
      *    日数
             ADD  RECE10-SHOKUJINISSU(1)  TO  SKT0400TMP-S-NISSU(IDX1)
                                              SKT0400TMP-S-GNISSU
      *    金額
             ADD  RECE10-SHOKUJIRYOYOHI(1)  TO
                                    SKT0400TMP-S-KINGK(IDX1)
                                    SKT0400TMP-S-GKINGK
      *    標準負担額
             ADD  RECE10-SHOKUJIFTN(1)  TO  SKT0400TMP-S-FTNGK(IDX1)
                                            SKT0400TMP-S-GFTNGK
      *
      *
           MOVE    1               TO  FLG-UPD 
           .
       2101-SYUKEI-EXT.
           EXIT.
      *
      *****************************************************************
      *    公費分集計処理
      *****************************************************************
       220-KOH-SYUKEI-SEC        SECTION.
      *
           PERFORM  VARYING  IDX3  FROM  1  BY  1
                    UNTIL    IDX3  >     4  OR
                    RECE10-KOHNUM(IDX3)  =  SPACE
               MOVE    ZERO                  TO  FLG-2010
      *
      *      請求書公費再掲欄記載情報があるとき
             IF  WRK-2010-MAX  >  ZERO
               PERFORM VARYING IDZ1    FROM    1   BY  1
                       UNTIL   IDZ1    >       WRK-2010-MAX
                 IF  RECE10-KOHNUM(IDX3)  =  WRK-2010-KOHNUM(IDZ1)
                   MOVE    1               TO  FLG-2010
                   MOVE    WRK-2010-MAX    TO  IDZ1
                 END-IF
               END-PERFORM
             END-IF
      *
             IF  FLG-2010  =  ZERO
      *
      *療養の給付
      *    件数
               ADD   1          TO  SKT0400TMP-KKENSU(IDX2)
      *    日数
               ADD   RECE10-JNISSU(IDX3 + 1) 
                                TO  SKT0400TMP-KNISSU(IDX2)
      *    点数
               ADD   RECE10-TOTALTEN(IDX3 + 1)
                                TO  SKT0400TMP-KTENSU(IDX2)
      *
      *食事療養
      *    件数
               IF  RECE10-SHOKUJINISSU(IDX3 + 1)  >  0
                 ADD  1  TO  SKT0400TMP-S-KKENSU
               END-IF
      *    日数
               ADD  RECE10-SHOKUJINISSU(IDX3 + 1)
                                TO  SKT0400TMP-S-KNISSU
      *    金額
               ADD  RECE10-SHOKUJIRYOYOHI(IDX3 + 1)
                                TO  SKT0400TMP-S-KKINGK
      *    標準負担額
               ADD  RECE10-SHOKUJIFTN(IDX3 + 1)
                                TO  SKT0400TMP-S-KFTNGK    
             END-IF
           END-PERFORM
      *
           .
       220-KOH-SYUKEI-EXT.
           EXIT.
