primary	{
	HOSPNUM,TEISYUTUSAKI,TEISYUTUSAKI2,HOSPCD,SYORIYM,RECEIPT_NO,PT_RENNUM;
};

path    key             {
        DBSELECT        {
                DECLARE tbl_henrei_body_key_csr CURSOR FOR
                SELECT *
                FROM tbl_henrei_body
                WHERE   HOSPNUM         = :HOSPNUM
                    AND NYUGAIKBN       = :NYUGAIKBN
                    AND SRYYM           = :SRYYM
                    AND PTID            = :PTID
                    AND PT_RENNUM       = 1 
                ;
        };
};

path    key2            {
        DBSELECT        {
                DECLARE tbl_henrei_body_key2_csr CURSOR FOR
                SELECT *
                FROM tbl_henrei_body
                WHERE     HOSPNUM         = :HOSPNUM
                    AND   NYUGAIKBN       = :NYUGAIKBN
                    AND   SRYYM           = :SRYYM
                    AND   PTID            = :PTID
                    AND  (TEISYUTUSAKI    =  :TEISYUTUSAKI  OR
                          TEISYUTUSAKI    =  :XXTEISYUTUSAKI)
                    AND    PT_RENNUM       = 1 
                    ORDER BY SYORIYM DESC,TEISYUTUSAKI2 DESC
                ;
        };
};

path    key3            {
        DBSELECT        {
                DECLARE tbl_henrei_body_key3_csr CURSOR FOR
                SELECT *
                FROM tbl_henrei_body
                WHERE     HOSPNUM         = :HOSPNUM
                    AND   TEISYUTUSAKI    = :TEISYUTUSAKI
                    AND   TEISYUTUSAKI2   = :TEISYUTUSAKI2
                    AND   SYORIYM         = :SYORIYM
                    AND   HOSPCD          = :HOSPCD
                    AND   RECEIPT_NO      = :RECEIPT_NO
                    AND   SELECT_FLG      = 1 
                    ORDER BY PT_RENNUM
                ;
        };
};

