path	key 	{
	DBSELECT	{
		DECLARE tbl_syunou_nyuin_key_csr CURSOR FOR
		SELECT *
		FROM tbl_syunou_nyuin
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   DENPNUM     =   :DENPNUM
		;
	};
};


path	del1 	{
	DBDELETE	{
		DELETE
		FROM tbl_syunou_nyuin
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   PTID        =   :PTID
		;
	};
};

path	del2 	{
	DBDELETE	{
		DELETE
		FROM tbl_syunou_nyuin
		WHERE (HOSPNUM,NYUGAIKBN,PTID,DENPNUM) in (
		SELECT HOSPNUM,NYUGAIKBN,PTID,DENPNUM
		FROM tbl_syunou_main
			WHERE   HOSPNUM       =   :HOSPNUM
			AND     NYUGAIKBN    =   :NYUGAIKBN
			AND     CREATEKBN    =   :SELCREATEKBN
		);
	};
};

