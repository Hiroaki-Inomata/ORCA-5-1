           PERFORM VARYING IDZ FROM    1   BY  1
                   UNTIL   IDZ >       4
             IF  RECE02-KOHNUM (IDZ) = "180" OR "182" OR "186"
                 PERFORM  20011-SYUKEI-SEC
             END-IF
           END-PERFORM
