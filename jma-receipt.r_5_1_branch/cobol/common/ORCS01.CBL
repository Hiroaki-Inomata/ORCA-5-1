                     IF  ( WK-SYUNOU-GENBUTU-JGNFLG  =  1    )
                     OR  ((COMB-KOH1HKNNUM  = "972" OR "974") AND
                          (COMB-KOH2HKNNUM  = SPACE         ) AND
                          (COMB-KOH3HKNNUM  = SPACE         ) AND
                          (COMB-KOH4HKNNUM  = SPACE         ))
