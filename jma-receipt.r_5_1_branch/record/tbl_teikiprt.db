path	del1 	{
	DBDELETE	{
		DELETE
		FROM tbl_teikiprt
        	WHERE   HOSPNUM     =   :HOSPNUM
		;
	};
};


path	del2 	{
	DBDELETE	{
		DELETE
		FROM tbl_teikiprt
        	WHERE   HOSPNUM     =   :HOSPNUM
        AND   DENPNUM     =   :DENPNUM
		AND		PTID        =   :PTID
		;
	};
};

path	upd1	{ 	
	DBUPDATE	{
		UPDATE	tbl_teikiprt
		SET	PTNUM       =   :PTNUM
                WHERE   HOSPNUM     =   :HOSPNUM
                  AND   PTID        =   :PTID
		;
	};
};

