tbl_dpc_jusho {
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	MDC_CD				char(2);
	BUNRUI_CD			char(4);
	JOKEN_KBN			char(2);
	JOKEN_NAME			varchar(50);
	JOKEN_MIN			number(3,0);
	JOKEN_MAX			number(3,0);
	JUSHO_CD			char(1);
	JUSHO_KBN			char(1);
	OPE_NAME			char(50);
	SEL_DPC_CD			varchar(14),virtual;
};

primary	{
	MDC_CD,BUNRUI_CD,JOKEN_KBN,JOKEN_MIN,JOKEN_MAX,JUSHO_CD,JUSHO_KBN,YUKOSTYMD,YUKOEDYMD;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_dpc_subbyo_key_csr CURSOR FOR
		SELECT *
		FROM tbl_dpc_jusho
		WHERE MDC_CD     =  :MDC_CD
		 AND  BUNRUI_CD  =  :BUNRUI_CD
		 AND  JOKEN_KBN  =  :JOKEN_KBN
		 AND  JOKEN_MIN  =  :JOKEN_MIN
		 AND  JOKEN_MAX  =  :JOKEN_MAX
		 AND  JUSHO_CD   =  :JUSHO_CD
		 AND  JUSHO_KBN  =  :JUSHO_KBN
	   	 AND  YUKOSTYMD     <=  :YUKOSTYMD
	   	 AND  YUKOEDYMD     >=  :YUKOEDYMD
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_dpc_jusho_key2_csr CURSOR FOR
		SELECT  a.*
		FROM tbl_dpc_jusho a ,
          ( SELECT JUSHO_CD FROM tbl_dpc_tensu
              WHERE DPC_CD     LIKE   :SEL_DPC_CD
               AND  YUKOSTYMD     <=  :YUKOSTYMD
               AND  YUKOEDYMD     >=  :YUKOEDYMD
            GROUP   BY JUSHO_CD
          ) b
		WHERE
		     A.MDC_CD     = :MDC_CD
		 AND A.BUNRUI_CD  = :BUNRUI_CD
		 AND A.JUSHO_CD   =  B.JUSHO_CD
	   	 AND A.YUKOSTYMD     <=  :YUKOSTYMD
	   	 AND A.YUKOEDYMD     >=  :YUKOEDYMD
	    ORDER BY A.JUSHO_CD,A.JUSHO_KBN DESC
		;
	};
};

path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_dpc_jusho_key3_csr CURSOR FOR
		SELECT  DISTINCT ON (JOKEN_KBN) a.*
		FROM tbl_dpc_jusho a ,
          ( SELECT JUSHO_CD FROM tbl_dpc_tensu
              WHERE MDC_CD     = :MDC_CD
               AND  BUNRUI_CD  = :BUNRUI_CD
               AND  YUKOSTYMD     <=  :YUKOSTYMD
               AND  YUKOEDYMD     >=  :YUKOEDYMD
            GROUP   BY JUSHO_CD
          ) b
		WHERE
		     A.MDC_CD     = :MDC_CD
		 AND A.BUNRUI_CD  = :BUNRUI_CD
		 AND A.JUSHO_CD   =  B.JUSHO_CD
	   	 AND A.YUKOSTYMD     <=  :YUKOSTYMD
	   	 AND A.YUKOEDYMD     >=  :YUKOEDYMD
	    ORDER BY A.JOKEN_KBN,A.JUSHO_CD,A.JUSHO_KBN
		;
	};
};

