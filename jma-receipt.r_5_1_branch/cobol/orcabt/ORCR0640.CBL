                     OR  (SYU-KOH-HKNNUM(3)  =
                          "012" OR "025" OR "972" OR "974")
                     OR  (SYU-KOH-HKNNUM(4)  =
                          "012" OR "025" OR "972" OR "974")
                       MOVE  1  TO  TBL-HKNCOMBI-CHKFLG(IDX1)
