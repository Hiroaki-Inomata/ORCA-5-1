primary	{
	HOSPNUM,PTID,SRYKA;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_clm_uid_key_csr CURSOR FOR
		SELECT *
		FROM tbl_clm_uid
			WHERE	HOSPNUM	=   :HOSPNUM
			AND	PTID	=   :PTID
			AND	SRYKA	=   :SRYKA
		;
	};
};


path	all 	{
	DBSELECT	{
		DECLARE tbl_clm_uid_all_csr CURSOR FOR
		SELECT *
		FROM tbl_clm_uid
			WHERE	HOSPNUM	=   :HOSPNUM
		;
	};
};

