                         (SYU-KOH-HKNNUM(3)  =  SPACE         ) AND
                         (SYU-KOH-HKNNUM(4)  =  SPACE         ))
                     OR ( SYU-KOH-HKNNUM(1)  =  "970"          )
                     OR ((SYU-KOH-HKNNUM(1)  =  "092"         ) AND
                         (SYU-NYNSHOKAISU    =    7           ) AND
                         (SYU-KOH-HKNNUM(2)  =  SPACE         ) AND
