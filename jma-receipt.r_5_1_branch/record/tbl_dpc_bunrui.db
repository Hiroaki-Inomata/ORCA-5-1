tbl_dpc_bunrui {
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	MDC_CD				char(2);
	BUNRUI_CD			char(4);
	BUNRUI_NAME			varchar(100);
};

primary	{
	MDC_CD,BUNRUI_CD,YUKOSTYMD,YUKOEDYMD;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_dpc_bunrui_key_csr CURSOR FOR
		SELECT *
		FROM tbl_dpc_bunrui
			WHERE MDC_CD        =   :MDC_CD
             AND  BUNRUI_CD     =   :BUNRUI_CD
		   	 AND  YUKOSTYMD     <=  :YUKOSTYMD
		   	 AND  YUKOEDYMD     >=  :YUKOEDYMD
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_dpc_bunrui_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_dpc_bunrui
			WHERE MDC_CD        =   :MDC_CD
		   	 AND  YUKOSTYMD     <=  :YUKOSTYMD
		   	 AND  YUKOEDYMD     >=  :YUKOEDYMD
            ORDER BY BUNRUI_CD
		;
	};
};

