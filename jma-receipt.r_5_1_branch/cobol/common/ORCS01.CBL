           AND (LNK-S01-IKTJISSIFLG    =  SPACE   )
           AND (WK-2010-CYOKI-FTNKBN   =  SPACE   )
           AND (LNK-SYU-KOHTAIYKZKBN   =  "1"     )
           AND (LNK-SYU-SRYYMD(1:6)   >=  "202001")
               IF  (LNK-SYU-SECMONEY   =  1 OR 2 OR 3 OR 4 OR 6)
               OR  (FLG-CYOKI-HANTEI2  =  ZERO                 )
                 CONTINUE
               ELSE
