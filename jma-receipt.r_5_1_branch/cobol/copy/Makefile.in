MCPAREA = \
	MCPAREA 


COBOL = @COBC@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
top_builddir = ../..

COPYS_inc = $(COPYS_sources:.INC.in=.INC)

.SUFFIXES: .INC.in .INC

all: MCPAREA $(COPYS_inc)

.INC.in.INC: 
	formdir_len=`echo -n "$(formdir)/"|wc -c|sed -e "s/ //g"` &&\
	recorddir_len=`echo -n "$(recorddir)/"|wc -c|sed -e "s/ //g"` &&\
	scriptsdir_len=`echo -n "$(scriptsdir)/"|wc -c|sed -e "s/ //g"` &&\
	allwaysscripts_len=`echo -n "$(allwaysscripts)/"|wc -c|sed -e "s/ //g"` &&\
	dailyscripts_len=`echo -n "$(dailyscripts)/"|wc -c|sed -e "s/ //g"` &&\
	monthlyscripts_len=`echo -n "$(monthlyscripts)/"|wc -c|sed -e "s/ //g"` &&\
	printscripts_len=`echo -n $(printscripts)|wc -c|sed -e "s/ //g"` &&\
	sed -e 's,\@prefix\@,$(prefix),g' \
	    -e 's,\@sitedir\@,$(SITEDIR),g' \
	    -e 's,\@patchdir\@,$(PATCHDIR),g' \
	    -e 's,\@formdir\@,$(formdir)/,g' \
	    -e 's,\@recorddir\@,$(recorddir)/,g' \
	    -e 's,\@scriptsdir\@,$(scriptsdir)/,g' \
	    -e 's,\@allwaysscripts\@,$(allwaysscripts)/,g' \
	    -e 's,\@dailyscripts\@,$(dailyscripts)/,g' \
	    -e 's,\@monthlyscripts\@,$(monthlyscripts)/,g' \
	    -e 's,\@printscripts\@,$(printscripts),g' \
	    -e "s,\@formdir_len\@,$$formdir_len,g" \
	    -e "s,\@recorddir_len\@,$$recorddir_len,g" \
	    -e "s,\@scriptsdir_len\@,$$scriptsdir_len,g" \
	    -e "s,\@allwaysscripts_len\@,$$allwaysscripts_len,g" \
	    -e "s,\@dailyscripts_len\@,$$dailyscripts_len,g" \
	    -e "s,\@monthlyscripts_len\@,$$monthlyscripts_len,g" \
	    -e "s,\@printscripts_len\@,$$printscripts_len,g" \
	  $< > $@

MCPAREA: $(top_builddir)/lddef/directory
	copygen -mcp  -dir $(top_builddir)/lddef/directory -record $(top_builddir)/record -ddir $(top_builddir)/lddef > $@ || (rm -f $@; false)
	test -s $@ || (rm -f $@; false)

clean: 
	rm -f $(COPYS_inc) $(MCPAREA)

distclean: clean
	-rm -f Makefile

filecheck:
	@(for i in $(COPYS) $(COPYS_sources) ;do echo "make	$${i}"; done ;\
	find . \
	-name "." -o \
	-name "CVS" -prune -o \
	-name "Makefile*" -o \
	-name ".cvsignore" -o \
	-name "*.bak" -o \
	-name "CPHOMEDIR.INC" -o \
	-name "CPCOMMONSHELL.INC" -o \
	-name "CPCOMMONSHELL1.INC" -o \
	-name "CPCOMMONSHELL4.INC" -o \
	-name "CPCOMMONSHELLMST.INC" -o \
	-name "MCPAREA" -o \
	-printf "file\t%f\n") \
	|sort -k2 |uniq -u -f1 

install: all installdirs

installdirs:

install-copys: install-copydirs
	for f in $(COPYS) $(COPYS_inc) $(MCPAREA); do \
	  $(INSTALL_DATA) $${f} $(copydir) ; done

install-copydirs:
	$(INSTALL) -d $(copydir)
