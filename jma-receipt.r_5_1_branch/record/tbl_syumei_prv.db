path	st_key3 	{
	DBSELECT	{
		DECLARE tbl_syumei_prv_st_key3_csr CURSOR FOR
		SELECT a.* FROM tbl_syumei_prv a,tbl_ptinf b
		WHERE    a.HOSPNUM  =   :HOSPNUM 
