query_teiki	{
	HOSPNUM				number(2,0);
	SRYYM				char(6);
	KBT					char(1);
	BTUNUM				char(2);
	BRMNUM				char(8);
	PTID				number(10,0);
	PTNUM				char(20);
	DENPNUM				number(7,0);
	KANANAME			char(100);
	KENSU				number(10,0);
	TENNYUYMD			char(8);
	TAIINYMD			char(8);
#-----> 検索キー用項目
	SELKBT1				char(1),virtual;
	SELKBT2				char(1),virtual;
	SELKBT3				char(1),virtual;
	SELTAIINYMD1		char(8),virtual;
	SELTAIINYMD2		char(8),virtual;
};

path	key		{ 	
	DBSELECT	{
		DECLARE query_teiki_key_csr CURSOR FOR
		SELECT  B.HOSPNUM,COUNT(*) AS KENSU
		FROM TBL_TEIKIPRT A,TBL_PTTEIKIRRK B,TBL_PTNYUINRRK C,TBL_PTINF D
		WHERE A.HOSPNUM  = :HOSPNUM
		AND   B.HOSPNUM  = A.HOSPNUM
		AND   B.PTID    = A.PTID
		AND   B.DENPNUM = A.DENPNUM
		AND   B.DELFLG  = 0
		AND   D.HOSPNUM  = A.HOSPNUM
		AND   D.PTID    = A.PTID
		AND   C.HOSPNUM  = A.HOSPNUM
		AND   C.PTID    = A.PTID
		AND   C.RRKNUM  = D.RRKNUM
		AND   C.RRKEDANUM  = D.RRKEDANUM
		AND ( C.TAIINYMD   LIKE  :SELTAIINYMD1
		 OR   C.TAIINYMD   LIKE  :SELTAIINYMD2 )
		GROUP BY B.HOSPNUM
		;
	};
};

