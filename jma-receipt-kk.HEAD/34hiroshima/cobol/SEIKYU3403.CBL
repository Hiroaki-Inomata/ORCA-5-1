      *****************************************************************
      *    請求点数集計処理
      *****************************************************************
       800-SEIKYU-SEC  SECTION.
      *
           IF  RECE02-RJNHKNNUM  =  SPACE
             IF  RECE02-FTNMONEY(1)  >  ZERO
               MOVE  SYU-SYUCOMPFTN         TO  WRK-SEIKYUGK
               ADD   SYU-SYUCOMPFTN         TO  MF100-ETC-FTNMONEY
             ELSE
               MOVE  SYU-SYUCOMPFTN-ENTANI  TO  WRK-SEIKYUGK
             END-IF
           ELSE
             IF  RECE02-FTNMONEY(1)  >  ZERO
               MOVE  SYU-KOH-COMPFTN(1)         TO  WRK-SEIKYUGK
               ADD   SYU-KOH-COMPFTN(1)         TO  MF100-ETC-FTNMONEY
             ELSE
               MOVE  SYU-KOH-COMPFTN-ENTANI(1)  TO  WRK-SEIKYUGK
             END-IF
           END-IF
      *
           PERFORM  VARYING  IDX  FROM  1  BY  1
                    UNTIL    IDX  >     4  OR
                    SYU-KOH-HKNNUM(IDX) =  SPACE
      *
             IF  SYU-KOH-HKNNUM(IDX)(1:1) =  "0" AND
                 SYU-KOH-HKNNUM(IDX) NOT  =  "027"
      *
      *    全国公費点数と全国公費一部負担金を集計する
      *    (一部負担金はレセプトに記載する場合のみ集計する)
               MOVE  SYU-KOH-HKNNUM(IDX)  TO  MF100-T-KOHNUM
               MOVE  COMB-KOHID(IDX)      TO  MF100-T-KOHID
               ADD  SYU-TOTAL-HKNTEN      TO  MF100-T-TOTALTEN
               IF (RECE02-FTNMONEY(1)      >  ZERO)  OR
                  (SYU-KOH-HKNNUM(IDX) NOT  =  "010" AND "011" AND
                                               "020" AND "021")
      *    特定疾患は負担割合が定められていないので、負担額を手動計算
                 IF  RECE02-FLG(2)         =  1
                 AND SYU-KOH-HKNNUM(IDX)   =  "051"
      *            保険点数の８割で計算する
                   COMPUTE WRK-KEISAN  =  SYU-TOTAL-HKNTEN * 10 * 0.2
                   ADD     WRK-KEISAN     TO  MF100-T-FTNMONEY
                 ELSE
                   ADD     SYU-KOH-COMPFTN(IDX)
                                            TO  MF100-T-FTNMONEY
                 END-IF
               END-IF
             END-IF
      *
      *    計算負担金に差が有る場合、地方公費の助成があったと判定する
             IF     FLG-ERR                 =  1
               IF   SYU-KOH-HKNNUM(IDX)     =  WRK-KOHNUM
                 IF     IDX                 =  1
                   IF  SYU-SYUCOMPFTN-ENTANI
                                        >  SYU-KOH-COMPFTN-ENTANI(IDX)
                     MOVE     ZERO         TO  FLG-ERR
                   END-IF
                 ELSE
                   IF  SYU-KOH-COMPFTN-ENTANI(IDX - 1)
                                        >  SYU-KOH-COMPFTN-ENTANI(IDX)
                     MOVE     ZERO         TO  FLG-ERR
                   END-IF
                 END-IF
               END-IF
             END-IF
      *
             IF  SYU-KOH-HKNNUM(IDX)  =   "972"
               MOVE     1            TO   FLG-TYOKI
             END-IF
             IF  SYU-KOH-HKNNUM(IDX)  =   "974"
               MOVE     2            TO   FLG-TYOKI
             END-IF
      *
           END-PERFORM
      *
           ADD     WRK-SEIKYUGK                     TO  WRK-SEIKYUGK-G
      *
