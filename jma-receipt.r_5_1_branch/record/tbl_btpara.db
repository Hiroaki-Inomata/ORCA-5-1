tbl_btpara {
	HOSPNUM				number(2,0);
	SHELLID 		char(8);
	RUN_OPID		char(16);
	DSP_RENNUM		number(3,0);
	RENNUM			number(4,0);
	JOBID			number(3,0);
	SCRIPTID		char(20);
	INFO_KBN		char(02);
	INFO_PARA		varchar(500);
	TERMID			varchar(16);
	OPID			varchar(16);
	CREYMD			char(8);
	UPYMD			char(8);
	UPHMS			char(6);
	SELLIMIT		number(8),virtual;
	SELOFFSET		number(8),virtual;
};

primary {
	HOSPNUM,SHELLID,RUN_OPID,DSP_RENNUM,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE 	HOSPNUM	= :HOSPNUM
			AND   SHELLID     =   :SHELLID    AND
					RUN_OPID    =   :RUN_OPID   AND
					DSP_RENNUM  =   :DSP_RENNUM AND
					RENNUM      =   :RENNUM
		;
	};
};

