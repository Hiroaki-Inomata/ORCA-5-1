path	key10	{
	DBSELECT	{
		DECLARE tbl_ptinf_prv_key10_csr CURSOR FOR
		SELECT distinct a.*,b.ptnum
		FROM tbl_jyurrk c,tbl_ptinf_prv a,tbl_ptnum b
			WHERE     c.HOSPNUM     =   :HOSPNUM
               AND    c.SRYYMD    >=  :UPYMD
               AND    c.SRYYMD    <=  :CREYMD
               AND    a.HOSPNUM    =   :HOSPNUM
               AND    a.PTID      =   c.PTID
               AND    b.HOSPNUM    =   :HOSPNUM
               AND    b.PTID      =   c.PTID
			ORDER BY  b.PTNUM
		;
	};
};
path	key11	{
	DBSELECT	{
		DECLARE tbl_ptinf_prv_key11_csr CURSOR FOR
		SELECT a.ptid from tbl_ptinf_prv a, 
		(SELECT distinct ptid from tbl_jyurrk 
		WHERE HOSPNUM	=	:HOSPNUM 
		AND (sryymd 	<=	:ED_SRYYMD 
		AND sryymd 	>=	:ST_SRYYMD) 
		AND nyugaikbn	=	:NYUGAIKBN 
		AND hknkbn	=	:HKNKBN 
		group by ptid) b 
		WHERE a.HOSPNUM	=	:HOSPNUM 
		AND a.ptid	=	b.ptid 
		AND a.tstptnumkbn	<>	'1'
		;
	};
};
