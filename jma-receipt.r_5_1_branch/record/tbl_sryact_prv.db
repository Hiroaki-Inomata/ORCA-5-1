};

path	key 	{
	DBSELECT	{
		DECLARE tbl_sryact_prv_key_csr CURSOR FOR
		SELECT *
		FROM tbl_sryact_prv
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYKA       =   :SRYKA
			AND   SRYYM       =   :SRYYM
			AND   ZAINUM      =   :ZAINUM
			AND   RENNUM      =   :RENNUM
		;
	};
};

