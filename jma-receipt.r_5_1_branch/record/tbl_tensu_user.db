path	key4 	{
	DBSELECT	{
		DECLARE tbl_tensu_user_key4_csr CURSOR FOR
		SELECT *,2
		FROM tbl_tensu_user
			WHERE HOSPNUM    =   :HOSPNUM
			AND   SRYCD      =   :SRYCD
			ORDER   BY  YUKOSTYMD
		;
	};
};

path	key27 	{
	DBSELECT	{
		DECLARE tbl_tensu_user_key27_csr CURSOR FOR
		SELECT *
		FROM tbl_tensu_user
			WHERE HOSPNUM    =   :HOSPNUM
			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
			AND ((SRYCD  >=   '001000987')  AND
                        (SRYCD  <=   '001999999')
                   OR   (SRYCD  >=   '002000006')  AND
                        (SRYCD  <=   '002999999')
                   OR   (SRYCD  LIKE '0082%')
                   OR   (SRYCD  LIKE '0083%')
                   OR   (SRYCD  LIKE '0084%')
                   OR   (SRYCD  LIKE '0085%')
                   OR   (SRYCD  LIKE '058%')
                   OR   (SRYCD  LIKE '059%')
                   OR   (SRYCD  LIKE '095%')
                   OR   (SRYCD  LIKE '096%'))
			ORDER   BY       SRYCD
		;
	};
};

path    del01    {
        DBDELETE        {
                DELETE
                FROM tbl_tensu_user
                        WHERE   HOSPNUM    =   :HOSPNUM
			AND   SRYCD    =   :SRYCD
                ;
        };
};

