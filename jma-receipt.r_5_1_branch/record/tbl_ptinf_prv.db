primary	{
	HOSPNUM,PTID;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_ptinf_prv_key_csr CURSOR FOR
		SELECT *
		FROM tbl_ptinf_prv
			WHERE   HOSPNUM      =   :HOSPNUM  AND
					PTID  		=   :PTID
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_ptinf_prv_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_ptinf_prv
			WHERE  (HOSPNUM    = :HOSPNUM) AND
					NAME   LIKE :NAME
			ORDER BY NAME
		;
	};
};
