<!ELEMENT mmlPc:rxRecord  (#PCDATA | mmlCm:extRef )*>
<!ELEMENT mmlPc:txRecord  (#PCDATA | mmlCm:extRef )*>
<!ELEMENT mmlPc:planNotes  (#PCDATA )>
