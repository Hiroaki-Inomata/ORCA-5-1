		SELECT a.*
		FROM tbl_nyuinacct a join tbl_ptnum b using (HOSPNUM,ptid) 
			WHERE a.HOSPNUM    =   :HOSPNUM
			AND   a.NYUGAIKBN   =   :NYUGAIKBN
