path	key4 	{
	DBSELECT	{
		DECLARE tbl_rece_putb_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_rece_putb
			WHERE   HOSPNUM		=   :HOSPNUM
			AND   PREFKBN		=   :PREFKBN
			AND   STYUKYMD		<=  :STYUKYMD
			AND   EDYUKYMD		>=  :EDYUKYMD
			ORDER BY SYUBETU_SRT2,KOUHI_NO
		;
	};
};

path    del1   {
        DBDELETE        {
                DELETE
                FROM tbl_rece_putb
			WHERE   HOSPNUM		=   :HOSPNUM
			AND     STYUKYMD	=   :STYUKYMD
			AND     EDYUKYMD	=   :EDYUKYMD
                ;
        };
};

path	all	{
	DBSELECT	{
		DECLARE tbl_rece_putb_all_csr CURSOR FOR
		SELECT *
		FROM tbl_rece_putb
		WHERE   HOSPNUM    =   :HOSPNUM
		;
	};
};


path    upd1    {
	DBUPDATE        {
		UPDATE  tbl_rece_putb
			SET
			STYUKYMD         =   :STYUKYMD,
			EDYUKYMD         =   :EDYUKYMD,
			TERMID           =   :TERMID,
			OPID             =   :OPID,
			UPYMD            =   :UPYMD,
			UPHMS            =   :UPHMS
		WHERE   STYUKYMD         =   :UPSTYUKYMD
		AND     EDYUKYMD         =   :UPEDYUKYMD
		AND     HOSPNUM          =   :HOSPNUM
		;
	};
};
