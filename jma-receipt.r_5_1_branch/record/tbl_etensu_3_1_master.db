tbl_etensu_3_1_master {
	SRYCD1				char(9);
	SRYCD2				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	HAIHAN				number(1,0);
	TOKUREI				number(1,0);
	CHGYMD   			char(8);
#-----> 主キー更新用項目
	UPSRYCD1			char(9),virtual;
	UPSRYCD2			char(9),virtual;
	UPYUKOSTYMD			char(8),virtual;
	UPYUKOEDYMD			char(8),virtual;
};
primary	{
	SRYCD1,SRYCD2,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_3_1_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_3_1
			WHERE   SRYCD1    =  :SRYCD1
			AND     SRYCD2    =  :SRYCD2
 			AND     YUKOSTYMD <= :YUKOSTYMD
			AND     YUKOEDYMD >= :YUKOEDYMD
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_etensu_3_1
			VALUES(
				:SRYCD1,
				:SRYCD2,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:HAIHAN,
				:TOKUREI,
				:CHGYMD
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_etensu_3_1
			SET
				SRYCD1    = :SRYCD1,
				SRYCD2    = :SRYCD2,
				YUKOSTYMD = :YUKOSTYMD,
				YUKOEDYMD = :YUKOEDYMD,
				HAIHAN    = :HAIHAN,
				TOKUREI   = :TOKUREI,
				CHGYMD    = :CHGYMD
			WHERE 	SRYCD1    = :UPSRYCD1
			AND     SRYCD2    = :UPSRYCD2
			AND	YUKOSTYMD = :UPYUKOSTYMD
			AND	YUKOEDYMD = :UPYUKOEDYMD
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_etensu_3_1
			WHERE   SRYCD1    = :SRYCD1
			AND     SRYCD2    = :SRYCD2
 			AND     YUKOSTYMD = :YUKOSTYMD
			AND     YUKOEDYMD = :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_3_1_master_key2_csr CURSOR FOR
		SELECT *
		FROM master.tbl_etensu_3_1
			WHERE   SRYCD1 = :SRYCD1
			AND     SRYCD2 = :SRYCD2
			ORDER   BY	YUKOSTYMD
		;
	};
};
