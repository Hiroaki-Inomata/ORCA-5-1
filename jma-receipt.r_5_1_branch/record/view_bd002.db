path	key 	{
	DBSELECT	{
		DECLARE view_bd002_key_csr CURSOR FOR
		SELECT *
		FROM view_bd002
			WHERE   HOSPNUM      =   :HOSPNUM
                    AND     DENPPRTYMD  =   :DENPPRTYMD
                    ORDER BY  PTNUM
		;
	};
};

path	key2 	{
	DBSELECT	{
