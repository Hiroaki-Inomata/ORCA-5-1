path	key4	{
	DBSELECT	{
		DECLARE tbl_inputset_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
                        AND     SETCD     >=  :SETCD
                        AND     SETCD     <=  :UPHMS
			ORDER   BY        SETCD,
                                          YUKOSTYMD,
			                  SETSEQ
		;
	};
};
path	key5	{
	DBSELECT	{
		DECLARE tbl_inputset_key5_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   SETSEQ      =   1
			ORDER   BY        YUKOSTYMD,
			                  SETSEQ
