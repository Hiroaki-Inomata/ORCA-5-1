path	key 	{
	DBSELECT	{
		DECLARE tbl_wksryact_key_csr CURSOR FOR
		SELECT *
		FROM tbl_wksryact
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYKA       =   :SRYKA
