tbl_hktsrycd {
	KANRICD   			number(3);
	RENNUM    			number(3);
	YUKOSTYMD  			char(8);
	YUKOEDYMD	  		char(8);
	SRYCD				char(9);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};

primary	{
	KANRICD,RENNUM,YUKOSTYMD,YUKOEDYMD;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_hktsrycd_key_csr CURSOR FOR
		SELECT *
		FROM tbl_hktsrycd
			WHERE   KANRICD     =   :KANRICD
			AND   RENNUM        =   :RENNUM
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_hktsrycd_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_hktsrycd
			WHERE   KANRICD     =   :KANRICD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			ORDER   BY         RENNUM
		;
	};
};

