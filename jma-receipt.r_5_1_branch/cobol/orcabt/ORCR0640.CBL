                     IF  (SYU-KOH-HKNNUM(1)  =  "092")
                     AND (SYU-NYNSHOKAISU    =    7  )
                       MOVE  1  TO  TBL-HKNCOMBI-CHKFLG2(IDX1)
                     END-IF
