tbl_rececom {
	HOSPNUM				number(2,0);
	PTID				number(10);
	NYUGAIKBN			char(1);
	SRYKA				char(2);
	SRYYM				char(6);
	HKNCOMBI			number(4);
	SRYDD				char(2);
	SJKBN				char(2);
	RENNUM				number(2);
	COMMENT        			varchar(4800);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD    			char(8);
	UPHMS				char(6);
#-----> 主キー更新用項目
	UPHOSPNUM			number(2,0),virtual;
	UPPTID 				number(10),virtual;
	UPNYUGAIKBN			char(1),virtual;
	UPSRYKA				char(2),virtual;
	UPSRYYM				char(6),virtual;
	UPHKNCOMBI			number(4),virtual;
	UPSRYDD				char(2),virtual;
	UPSJKBN				char(2),virtual;
	UPRENNUM			number(2),virtual;
};
primary	{
	HOSPNUM,PTID,NYUGAIKBN,SRYKA,SRYYM,HKNCOMBI,SRYDD,SJKBN,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_rececom_key_csr CURSOR FOR
		SELECT *
		FROM tbl_rececom
			WHERE   HOSPNUM    =   :HOSPNUM
			AND     PTID      =   :PTID
			AND     NYUGAIKBN =   :NYUGAIKBN
			AND     SRYKA     =   :SRYKA
			AND     SRYYM     =   :SRYYM
			AND     HKNCOMBI  =   :HKNCOMBI
			AND     SRYDD     =   :SRYDD
			AND     SJKBN     =   :SJKBN
			AND     RENNUM    =   :RENNUM
		;
	};
};

