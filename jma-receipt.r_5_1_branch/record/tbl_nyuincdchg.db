tbl_nyuincdchg {
	SRYCD_BF   		char(9);
	SRYCD_AF   		char(9);
	YUKOSTYMD	  	char(8);
	YUKOEDYMD	  	char(8);
	NYUINRYOCD  	char(3);
	TERMID  		varchar(16);
	OPID    		varchar(16);
	CREYMD  		char(8);
	UPYMD   		char(8);
	UPHMS			char(6);
};
primary	{
	SRYCD_BF,SRYCD_AF,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_nyuincdchg_key_csr CURSOR FOR
		SELECT *
		FROM tbl_nyuincdchg
			WHERE SRYCD_BF  =   :SRYCD_BF
			AND   SRYCD_AF  =   :SRYCD_AF
			AND   YUKOSTYMD <=  :YUKOSTYMD
			AND   YUKOEDYMD >=  :YUKOEDYMD
		;
	};
};

