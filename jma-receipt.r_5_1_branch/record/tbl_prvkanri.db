tbl_prvkanri	{
	HOSPNUM				number(2,0);
	TBL_KEY			char(8);
	RENNUM			number(4);
	TBL_GROUP		char(14);
	TBL_UUID		char(36);
	SHORI_RENNUM		number(4);
	GYOUMU_CD               number(3);
	SHELLID			varchar(20);
	PRTID			varchar(20);
	SRYYM			varchar(06);
	SKYYMD  		varchar(08);
	PTID			number(10);
	PAGE	   		number(5);
	MOD_FLG			char(1);
	SHORI_MOD_FLG		char(1);
	FILENAME		varchar(200);
	TITLE			varchar(100);
	PRTNM	  		varchar(16);
	PTNUM			varchar(20);
	MEMO 			varchar(100);
	RECE_FLG 		number(4);
	SHORI_KBN		char(1);
	TBL_GROUP1		char(14);
	TBL_GROUP2		char(14);
	TBL_GROUP3		char(14);
	TBL_GROUP4		char(14);
	TBL_GROUP5		char(14);
	TERMID			varchar(32);
	OPID			varchar(16);
	CREYMD			varchar(08);
	UPYMD			varchar(08);
	UPHMS			varchar(06);
#-----> 検索用項目
	STPAGE			number(5),virtual;
	EDPAGE			number(5),virtual;
};

primary	{
	HOSPNUM,TBL_KEY,RENNUM,TBL_GROUP,TBL_UUID,SHORI_RENNUM,PAGE;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_prvkanri_key_csr CURSOR FOR
		SELECT *
		FROM tbl_prvkanri
		WHERE 	HOSPNUM	= :HOSPNUM
                    AND   TBL_KEY	=	:TBL_KEY
                    AND     RENNUM       =   :RENNUM
                    AND     TBL_GROUP    =   :TBL_GROUP
                    AND     TBL_UUID     =   :TBL_UUID
                    AND     SHORI_RENNUM =   :SHORI_RENNUM 
                    AND     PAGE         =   :PAGE 
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_prvkanri_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_prvkanri
		WHERE 	HOSPNUM	= :HOSPNUM
