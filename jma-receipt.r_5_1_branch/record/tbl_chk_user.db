path	upd1	{
	DBUPDATE	{
		UPDATE	tbl_chk_user
			SET
				HOSPNUM      = :HOSPNUM,
				CHKKBN       = :CHKKBN,
				SRYCD        = :SRYCD,
				GRPNUM       = :GRPNUM,
				YUKOSTYMD    = :YUKOSTYMD,
				YUKOEDYMD    = :YUKOEDYMD,
				CDKBN        = :CDKBN,
				CD           = :CD,
				NYUGAIKBN    = :NYUGAIKBN,
				NYUGAIBNDKBN = :NYUGAIBNDKBN,
				SYORIKBN     = :SYORIKBN,
				CHKERR       = :CHKERR,
				DATAKBN      = :DATAKBN,
				RENNUM       = :RENNUM,
				TERMID       = :TERMID,
				OPID         = :OPID,
				CREYMD       = :CREYMD,
				UPYMD        = :UPYMD,
				UPHMS        = :UPHMS
		WHERE	HOSPNUM = :UPHOSPNUM
			AND CHKKBN    = :UPCHKKBN
			AND SRYCD     = :UPSRYCD
			AND GRPNUM    = :UPGRPNUM
			AND YUKOSTYMD = :UPYUKOSTYMD
			AND YUKOEDYMD = :UPYUKOEDYMD
			AND CDKBN     = :UPCDKBN
			AND CD        = :CD
		;
	};
};

