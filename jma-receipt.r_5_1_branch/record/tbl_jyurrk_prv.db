path	st_key1 	{
	DBSELECT	{
		DECLARE tbl_jyurrk_prv_st_key1_csr CURSOR FOR
		SELECT a.* FROM tbl_jyurrk_prv a,tbl_ptinf b
		WHERE    a.HOSPNUM  =  :HOSPNUM
		AND   a.NYUGAIKBN  =  :NYUGAIKBN
		AND   (a.SRYYMD    >= :SRYYMD
		AND    a.SRYYMD    <= :UPSRYYMD)
		AND   a.HOSPNUM     =  b.HOSPNUM
		AND   a.PTID       =  b.PTID
		AND   b.TSTPTNUMKBN  <> '1'
		ORDER  BY   a.PTID,
		a.SRYYMD,
		a.RENNUM,
		a.DOUJI_RENNNUM,
		a.EDANUM
		;
	};
};

