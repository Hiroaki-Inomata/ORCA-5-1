           OR   ( RECE62-KOHNUM(2)  =   "974" )
           OR   ( RECE62-KOHNUM(3)  =   "974" )
           OR   ( RECE62-KOHNUM(4)  =   "974" )
              IF   WRK-KOH-GENDOGAKU  >  20000
