tbl_tekioubyomei {
	CHKKBN			char(1);
	SRYCD			char(9);
	RENNUM			number(4);
	BYOMEI			varchar(400);
	BYOMEICDSU		number(2);
	BYOMEICD_1		char(7);
	BYOMEICD_2		char(7);
	BYOMEICD_3		char(7);
	BYOMEICD_4		char(7);
	BYOMEICD_5		char(7);
	BYOMEICD_6		char(7);
	BYOMEICD_7		char(7);
	BYOMEICD_8		char(7);
	BYOMEICD_9		char(7);
	BYOMEICD_10		char(7);
	BYOMEICD_11		char(7);
	BYOMEICD_12		char(7);
	BYOMEICD_13		char(7);
	BYOMEICD_14		char(7);
	BYOMEICD_15		char(7);
	BYOMEICD_16		char(7);
	BYOMEICD_17		char(7);
	BYOMEICD_18		char(7);
	BYOMEICD_19		char(7);
	BYOMEICD_20		char(7);
	BYOMEICD_21		char(7);
	BYOMEIHENFLG	char(1);
	KHNBYOMEICD		char(7);
	KHNBUICD_1		char(7);
	KHNBUICD_2		char(7);
	KHNBUICD_3		char(7);
	TERMID			varchar(32);
	OPID			varchar(16);
	CREYMD			char(8);
	UPYMD			char(8);
	UPHMS			char(6);
#-----> 検索キー用項目
	SELNAME		char(200),virtual;
	SELYUKOSTYMD  		char(8),virtual;
	SELYUKOEDYMD  		char(8),virtual;
};
primary	{
	CHKKBN,SRYCD,RENNUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_tekioubyomei_key_csr CURSOR FOR
		SELECT *
		FROM tbl_tekioubyomei
			WHERE   CHKKBN	=	:CHKKBN
			AND		SRYCD	=	:SRYCD
			AND		RENNUM	=	:RENNUM
		;
	};
};
path	key2 	{
	DBSELECT	{
		DECLARE tbl_tekioubyomei_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_tekioubyomei
			WHERE   CHKKBN	=	:CHKKBN
			AND		SRYCD	LIKE :SRYCD
            ORDER BY CHKKBN,SRYCD,BYOMEI ;
	};
};

path	key3 	{
	DBSELECT	{
		DECLARE tbl_tekioubyomei_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_tekioubyomei
			WHERE CHKKBN		=	:CHKKBN
			AND   SRYCD			=	:SRYCD
			AND   BYOMEI       LIKE :BYOMEI
		;
	};
};

path	key4	{ 	
	DBSELECT	{
		DECLARE tbl_tekioubyomei_key4_csr CURSOR FOR
		SELECT A.* FROM tbl_tekioubyomei A,TBL_TENSU B
		WHERE  A.CHKKBN	       = :CHKKBN
		AND    A.RENNUM	       = 1
		AND    B.SRYCD         = A.SRYCD
		AND    B.YUKOSTYMD    <= :SELYUKOSTYMD
		AND    B.YUKOEDYMD    >= :SELYUKOEDYMD
		AND	(B.NAME      LIKE :SELNAME
		 OR	 B.KANANAME  LIKE :SELNAME)
		ORDER BY B.KANANAME,A.SRYCD
		;
	};
};

path	all 	{
	DBSELECT	{
		DECLARE tbl_tekioubyomei_all_csr CURSOR FOR
		SELECT *
		FROM tbl_tekioubyomei
		;
	};
};
