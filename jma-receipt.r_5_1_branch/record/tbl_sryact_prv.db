path	st_key4 	{
	DBSELECT	{
		DECLARE tbl_sryact_prv_st_key4_csr CURSOR FOR
		SELECT a.*
		FROM tbl_sryact_prv a,tbl_ptinf b
		WHERE   a.HOSPNUM        =  :HOSPNUM
		AND     a.SRYYM         =  :SRYYM
		AND     a.HOSPNUM        =  b.HOSPNUM
		AND     a.PTID          =  b.PTID
		AND     b.TSTPTNUMKBN  <> '1'
		ORDER   BY a.PTID,a.SRYKA,a.ZAINUM
		;
	};
};

